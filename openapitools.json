{
    "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
    "spaces": 2,
    "generator-cli": {
        "version": "6.5.0",
        "generators": {
            "typescript": {
                "generatorName": "typescript-axios",
                "output": "#{cwd}/generated/",
                "inputSpec": "http://localhost:5000/api-json",
                "globalProperty": "models",
                "skipValidateSpec": true,
                "apiPackage": "api",
                "modelPackage": "dto",
                "additionalProperties": {
                    "supportsES6": true,
                    "withInterfaces": true,
                    "withSeparateModelsAndApi": true,
                    "stringEnums": true,
                    "withoutPrefixEnums": true
                }
            }
        }
    }
}